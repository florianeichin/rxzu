{
  "npmScope": "rxzu",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "core": {
      "tags": []
    },
    "angular": {
      "tags": [],
      "implicitDependencies": ["core"]
    },
    "angular-storybook": {
      "tags": []
    },
    "angular-e2e": {
      "tags": [],
      "implicitDependencies": ["core", "angular"]
    },
    "react": {
      "tags": []
    }
  },
  "workspaceLayout": {
    "libsDir": "packages"
  }
}
